page{
  width: 100%;
  height: 100%;
  background-color: #b3d4db;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center; 
}
.user-avatar{
  width: 200rpx;
  height: 200rpx;
  margin-top: 300rpx;
}
.user-name{
  font-size: 32rpx;
  font-weight: bold;
  margin: 80rpx 0;
}
.moto-container{
  border: 1px solid #405f80;
  width: 200rpx;
  height: 80rpx;
  border-radius: 8rpx;
  text-align: center;
}
.moto-container .moto{
  color: #405f80;
  font-size: 22rpx;
  font-weight: bold;
  line-height: 80rpx;
}